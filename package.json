{
  "name": "v-phone-input",
  "type": "module",
  "version": "5.1.3",
  "license": "MIT",
  "description": "International phone field for Vuetify 3 and Vue 3.",
  "homepage": "https://paul-thebaud.github.io/v-phone-input/",
  "repository": {
    "type": "git",
    "url": "https://github.com/paul-thebaud/v-phone-input.git"
  },
  "bugs": {
    "url": "https://github.com/paul-thebaud/v-phone-input/issues"
  },
  "keywords": [
    "vue",
    "vuetify",
    "telephone",
    "phone",
    "phone input",
    "telephone input",
    "input",
    "international phone",
    "vue 3",
    "vuetify 3"
  ],
  "files": [
    "dist/*",
    "src/**/*.vue"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/v-phone-input.js",
      "require": "./dist/v-phone-input.umd.cjs"
    },
    "./styles": "./dist/v-phone-input.css"
  },
  "types": "dist/index.d.ts",
  "module": "dist/v-phone-input.js",
  "main": "dist/v-phone-input.umd.cjs",
  "style": "dist/v-phone-input.css",
  "sideEffects": [
    "*.vue"
  ],
  "scripts": {
    "lint": "biome check src docs/components",
    "test": "vitest run --coverage --typecheck",
    "build": "vue-tsc --noEmit && vite build",
    "docs:typedoc": "typedoc && rimraf docs/api/_media docs/api/index.md && cp -RT docs/api-handwritten docs/api",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "dependencies": {
    "awesome-phonenumber": "^7.6.0",
    "countries-list": "^3.2.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.6",
    "@cypress/code-coverage": "^3.14.7",
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@mdi/font": "^7.4.47",
    "@mdi/js": "^7.4.47",
    "@types/markdown-it": "^14.1.2",
    "@vitejs/plugin-vue": "^6.0.2",
    "@vitest/coverage-v8": "^4.0.12",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.8.1",
    "cypress": "^15.7.0",
    "markdown-it": "^14.1.0",
    "rimraf": "^6.1.2",
    "typedoc": "^0.28.14",
    "typedoc-plugin-markdown": "^4.9.0",
    "typedoc-plugin-mdn-links": "^5.0.10",
    "typedoc-vitepress-theme": "^1.1.2",
    "typescript": "^5.9.3",
    "vite": "^7.2.4",
    "vite-plugin-dts": "^4.5.4",
    "vite-plugin-istanbul": "^7.2.1",
    "vite-plugin-vuetify": "^2.1.2",
    "vitepress": "2.0.0-alpha.12",
    "vitepress-plugin-group-icons": "^1.6.5",
    "vitest": "^4.0.12",
    "vue": "^3.5.24",
    "vue-component-type-helpers": "^3.1.4",
    "vue-tsc": "^3.1.4",
    "vuetify": "^3.10.4"
  },
  "peerDependencies": {
    "vue": "^3.5.0"
  },
  "optionalDependencies": {
    "flag-icons": "^7.5.0",
    "vuetify": "^3.10.11",
    "world-flags-sprite": "^0.0.2"
  },
  "engines": {
    "node": ">=12"
  }
}
